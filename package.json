{
  "name": "@caeljs/jsx",
  "type": "module",
  "description": "ASYNC JSX/SSR framework for building modern web applications with component-based templates. It offers a lightweight virtual DOM, intelligent routing system, and reactive components with no external dependencies. Designed to generate optimized static websites or dynamic SSR applications with high performance",
  "version": "0.1.1",
  "main": "cjs/JsxSSR.cjs",
  "module": "esm/JsxSSR.js",
  "types": "dist/JsxSSR.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./JsxSSR.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./JsxSSR.d.ts",
      "import": "./esm/JsxSSR.js",
      "require": "./cjs/JsxSSR.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drylian/JsxJS.git"
  },
  "keywords": [
    "jsx",
    "async",
    "ssr",
    "dom",
    "router",
    "reactive",
    "framework",
    "lightweight",
    "spa",
    "static"
  ],
  "author": "Drylian",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/drylian/JsxJS/issues"
  },
  "homepage": "https://github.com/drylian/JsxJS#readme",
  "scripts": {
    "build": "bun build.ts"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "dts-bundle-generator": "^9.5.1",
    "tsup": "^8.3.5",
    "typescript": "^5.3.3"
  }
}
